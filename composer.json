{
    "name": "rayzenai/filament-snapshots",
    "description": "A Laravel package for managing content snapshots with restore functionality in Filament",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "snapshots",
        "content",
        "restore",
        "versioning"
    ],
    "authors": [
        {
            "name": "Kiran Timsina",
            "email": "timsinakiran@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "filament/filament": "^3.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "CmsMax\\FilamentSnapshots\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CmsMax\\FilamentSnapshots\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CmsMax\\FilamentSnapshots\\FilamentSnapshotsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}